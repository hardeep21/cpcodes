#include<bits/stdc++.h>
using namespace std;

void minmaxele(int arr[],int n)
{
    int max = arr[0], min = arr[0];
    for(int i = 0;i<n;i++)
    {
      if(arr[i]>max)
            max=arr[i];
      if(arr[i]<min)
            min=arr[i];
    }
    cout<<"minimum: "<<min<<'\n'<<"maximum: "<<max;
}

int main()
{
    int n;
    cin>>n;
    int arr[100];
    for(int i = 0; i<n; i++)
    {
        cin>>arr[i];
    }
    minmaxele(arr,n);
    return 0;
}
